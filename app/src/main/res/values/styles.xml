<resources xmlns:tools="http://schemas.android.com/tools">

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.MaterialComponents.Light.NoActionBar">

        <!-- Customize your theme here. -->
        <item name="colorPrimaryDark">@color/color_Deer</item>
        <item name="colorPrimary">@color/color_DesertSand</item>
        <item name="colorAccent">@color/color_FuzzyWuzzy</item>
        <item name="colorControlHighlight">@color/color_FuzzyWuzzy</item>
        <item name="android:preferenceStyle">@style/PreferenceThemeOverlay.v14.Material</item>
    </style>
    <style name="AppTheme.launcher">
        <item name="android:windowIsTranslucent">true</item>
        <item name="colorPrimaryDark">#fff</item>
        <item name="android:windowLightStatusBar">true</item>
    </style>
    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

</resources>
