<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:width="4dp"
        android:height="30dp"
        android:bottom="1dp">
        <shape xmlns:android="http://schemas.android.com/apk/res/android">
            <solid
                android:color="@color/color_GreyInde" />
        </shape>
    </item>
    <item
        android:height="6dp"
        android:width="10dp"
        android:top="12dp">
        <shape
            android:shape="rectangle">
            <solid
                android:color="@color/color_DesertSand"/>
            <corners
                android:radius="24dp"
                />
        </shape>
    </item>
</layer-list>